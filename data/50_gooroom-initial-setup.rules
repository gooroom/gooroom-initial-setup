// DO NOT EDIT THIS FILE, it will be overwritten on update.
//
// Allow the gis user to do certain actions without
// being interrupted by password dialogs

polkit.addRule(function(action, subject) {
    if (subject.user !== 'gis')
        return undefined;

    var actionMatches = (action.id.indexOf('org.freedesktop.NetworkManager.') === 0);

    if (actionMatches) {
        if (subject.local)
            return 'yes';
        else
            return 'auth_admin';
    }

    return undefined;
});
